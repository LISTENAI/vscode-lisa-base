{
	"name": "lisa-base",
	"displayName": "LISTENAI官方发布的IDE基础插件。",
	"description": "lisa-base",
	"version": "0.0.8",
	"publisher": "listenai",
	"icon": "lisa.png",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://git.iflyos.cn/ListenAI/vscode-extensions/lisa.git"
	},
	"activationEvents": [
		"onStartupFinished",
		"onView:lisa.tree",
		"onCommand:lisa.command",
		"onCommand:lisa.base.check"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "lisa.refreshEntry",
				"title": "Refresh",
				"icon": {
					"dark": "./assets/icons/r_refresh_white.svg",
					"light": "./assets/icons/r_refresh_white.svg"
				}
			},
			{
				"command": "lisa.command",
				"title": "lisa命令执行"
			},
			{
				"command": "lisa.base.check",
				"title": "lisa-base检查"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "lisa",
					"title": "工程快捷菜单",
					"icon": "./assets/icons/Icon_commands.svg"
				}
			],
			"panel": []
		},
		"views": {
			"lisa": [
				{
					"id": "lisa.tree",
					"name": "工程快捷菜单"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "lisa.refreshEntry",
					"when": "view == lisa.tree",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/configstore": "^5.0.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": " ^1.46.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"configstore": "^5.0.1"
	}
}
